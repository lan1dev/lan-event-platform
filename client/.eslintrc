{
  "extends": [
    "airbnb",
    "prettier"
    ],
  "plugins": [
    "prettier"
  ],
  "rules": {
        "jsx-a11y/anchor-is-valid": [ "error", {
            "components": []
          }],
        "react/jsx-one-expression-per-line": 0,
        "prettier/prettier": "error",
    },
  "env":{
    "browser": true,
    "node": true,
    "jest": true,
    },
    "parser": "babel-eslint"
}
